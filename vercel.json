{
    "version": 2,
    "framework": null,
    // "functions": {
    //     "api/index.php": { "runtime": "vercel-php@0.7.4" }
    // },
    "builds": [
        { "src": "public/**", "use": "@vercel/static" },
        { "src": "api/index.php", "use": "vercel-php@0.7.4" }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|favicon)/(.)",
            "dest": "/public/$1/$2"
        },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_NAME": "Luarsekolah",
        "APP_ENV": "production",
        "APP_KEY": "base64:hWlQjiaMIa+Ce8n/LOs7CVw+YGSx1xma9zhTnxP41yY=",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourprojectdomain.com",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "APP_LOCALE": "en",
        "APP_FALLBACK_LOCALE": "en",
        "APP_FAKER_LOCALE": "en_US",

        "BCRYPT_ROUNDS": "12",

        "DB_CONNECTION": "sqlite",

        "VITE_APP_NAME": "${APP_NAME}"
    }
}
